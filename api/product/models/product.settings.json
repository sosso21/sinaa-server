{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "float"
    },
    "categories": {
      "via": "products",
      "collection": "category"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ],
      "default": "published",
      "required": true
    },
    "view": {
      "type": "integer",
      "default": 0
    },
    "like": {
      "type": "integer",
      "default": 0
    },
    "like_user": {
      "type": "component",
      "repeatable": false,
      "component": "custom.like-user"
    },
    "search": {
      "type": "integer",
      "default": 0
    },
    "lastConnect": {
      "type": "datetime"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "adress": {
      "type": "component",
      "repeatable": false,
      "component": "custom.adress"
    },
    "images": {
      "type": "component",
      "repeatable": false,
      "component": "custom.images"
    },
    "sector": {
      "type": "string"
    },
    "profession": {
      "type": "string"
    },
    "society": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "number": {
      "type": "integer"
    },
    "type_work": {
      "type": "enumeration",
      "enum": [
        "regional",
        "national"
      ]
    },
    "salary_min": {
      "type": "integer"
    },
    "salary_max": {
      "type": "integer"
    },
    "Mark": {
      "type": "string"
    },
    "typestate": {
      "type": "enumeration",
      "enum": [
        "new",
        "almost_new",
        "second_hand",
        "rental"
      ]
    },
    "formation_category": {
      "type": "string"
    }
  }
}
